{
  "rules": [
    {
      "description": "Command is Command+space if tapped, Command if held down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_command",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "spacebar",
              "modifiers": [
                "left_command"
              ]
            }
          ]
        }
      ]
    }
  ],
  "title": "Command like Super key in Gnome"
}


